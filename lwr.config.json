{
    "lwc": { "modules": [{ "dir": "src/modules" }] },
    "routes": [
        {
            "id": "example",
            "path": "/",
            "rootComponent": "example/app"
        }
    ],
    "assets": [
        {
            "alias": "assetsDir",
            "dir": "src/assets",
            "urlPath": "/public/assets"
        },
        {
            "alias": "favicon",
            "file": "src/assets/favicon.ico",
            "urlPath": "/favicon.ico"
        }
    ]
}
